<MainWindow>
    name: "main"
    GridLayout:
        cols:1
        GridLayout:
            cols: 2

            Label:
                text: "password: "
            TextInput:
                id: passw
                multiline: False
        Button:
            text: "enter"
            on_release:
                app.root.current = "second"
                root.manager.transition.direction = "left"


<SecondWindow>
    name: "second"
    sname: sname
    email: email

    Button:
        text: "G0 Back"
        size_hint: 1, 0.3
        background_color: 0.9,0.3,0.5,1
        on_release:
            app.root.current = "fourth"
            root.manager.transition.direction = "left"




    Button:
        pos_hint: { "x":0.11, "down":1}
        size_hint: 0.1, 0.1
        text: "11" if btn.state == "normal" else "Down"
        background_color: 0.8,0.4,0.5,1




    GridLayout:
        cols:1
        size: root.width, root.height
        pos: 10, 10

        GridLayout:
            cols:2


            Label:
                text:"Name: "

            TextInput:
                id: sname
                multiline:True
                text: root.showtext()
                pos_hint: {"x":0.1}
                background_color: 0.3,0.4,0.5,1
                size_hint:10, 20

            Label:
                text: "Email: "

            TextInput:
                id: email
                multiline:False
                pos_hint: {"x":0.1}
                background_color: 0.3,0.4,0.5,1
                size_hint:10, 20

        Button:
            text:"Submit"
            on_press: root.btn()


            pos_hint: {"x":0.1}
            background_color: 0.3,0.4,0.5,1
            size_hint:0.1, 0.1


























    Button:
