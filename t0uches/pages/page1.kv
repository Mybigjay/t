    Button:
        id: btn
        size_hint: 0.1, 0.1
        pos_hint: { "x":0.01, "top":1}
        text: "1" if btn.state == "normal" else "Down"
        background_color: 0.1,0.4,0.5,1
        on_release:
            app.root.current = "main"
            root.manager.transition.direction = "left"
    Button:

        id: btn
        size_hint: 0.1, 0.1
        pos_hint: { "x":0.11, "top":1}

        padding: 50, 40
        spacing: 100

        text: "2" if btn.state == "normal" else "Down"
        background_color: 0.1,0.4,0.5,1
        on_release:
            app.root.current = "main"
            root.manager.transition.direction = "left"


